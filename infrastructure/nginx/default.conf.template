charset utf-8;
tcp_nopush on;
tcp_nodelay on;
types_hash_max_size 2048;
client_max_body_size 100M;
server_tokens off;

# Gzip Settings
gzip on;
gzip_vary on;
gzip_proxied any;
gzip_comp_level 6;
gzip_min_length 256;
gzip_types
    text/plain
    text/css
    text/xml
    application/json
    application/javascript
    application/xml
    application/xml+rss
    image/svg+xml;